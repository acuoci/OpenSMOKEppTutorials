Dictionary BatchReactor
{
		@KineticsPreProcessor	POLIMI_H2CO_NOX_1407;
		
		@Type					Adiabatic-ConstantVolume;
		
		@InitialStatus          Initial-Composition;
		
		@EndTime				0.001 s;
		
		@SensitivityAnalysis	sensitivity-options;
}

Dictionary POLIMI_H2CO_NOX_1407
{
		@Kinetics			..\..\KineticMechanisms\POLIMI_1407\Kinetics\POLIMI_H2CO\POLIMI_H2CO_NOX_1407.CKI;
		@Thermodynamics		..\..\KineticMechanisms\POLIMI_1407\Thermodynamics\POLIMI_TOT_NOX_1407.CKT;
		@Output				kinetics;
}

Dictionary Initial-Composition
{
		@Temperature   	1000.   K ; 
		@Pressure      	101325. Pa ;

		@EquivalenceRatio 		1.;	
		@FuelMoleFractions 		H2 1.;
		@OxidizerMoleFractions 	O2 0.21 N2 0.79;
}

Dictionary sensitivity-options
{	
	@Type 				arrhenius-parameters; 
	@FullPivoting 		false;
	@LinearAlgebra 		Eigen;
	@Species 			H2 O2 NO;
	@SubSteps 			5;
}


