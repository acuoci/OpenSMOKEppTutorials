Dictionary SurfaceBatchReactor
{
		@KineticsFolder		../../OpenSMOKEpp_CHEMKIN_PreProcessor/14-catalytic-deutschmann/deutschmann-17/kinetics-Deutschmann_WGS_over_Rh;
		
		@Type					Isothermal-ConstantVolume;
		
		@InitialStatus          initial-mixture;
		
		@EndTime				1 s;
		
		@Volume					100 cm3;
		@InternalSurfaceArea	10 cm2;
		
		@OdeParameters          ode-parameters;
}

Dictionary initial-mixture
{
		@MoleFractions			CO2		0.10
								H2		0.10
								CO		0.10
								O2		0.10
								N2		0.60;
								
		@Temperature			700 K;
		@Pressure				1	atm;
}

Dictionary Surface-Rh_surface
{
		@SurfaceFractions	Rh(s)	1.00;
}

Dictionary ode-parameters
{	
		@OdeSolver OpenSMOKE;
		@AbsoluteTolerance 1e-16;
		@RelativeTolerance 1e-8;
}


