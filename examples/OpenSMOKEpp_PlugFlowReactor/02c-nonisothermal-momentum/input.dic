Dictionary PlugFlowReactor
{
		@KineticsPreProcessor	POLIMI_H2CO_NOX_1412;
		//@KineticsFolder			kinetics;
		
		@Type					NonIsothermal;
		
		@InletStatus          	inlet-mixture;
		
		@Length					1000 cm;
		
		@ConstantPressure			false;
		@MomentumEquation			true;
		@CrossSectionOverPerimeter	0.05 cm;
		
		@Velocity				1 m/s;
}

Dictionary POLIMI_H2CO_NOX_1412
{
		@Kinetics		../../../kinetic-mechanisms/POLIMI_1412/Kinetics/POLIMI_TOT_NOX_1412.CKI;
		@Thermodynamics	../../../kinetic-mechanisms/POLIMI_1412/Thermodynamics/POLIMI_TOT_NOX_1412.CKT;
		@Output			kinetics;
}

Dictionary inlet-mixture
{
		@Temperature   	1000.  	K ; 
		@Pressure      	101325 	Pa;
		@Moles			H2 2 
						O2 1 
						N2 3.76 ;
}
