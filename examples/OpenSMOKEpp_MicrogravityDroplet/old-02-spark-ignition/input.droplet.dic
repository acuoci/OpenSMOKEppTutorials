Dictionary MicrogravityDroplet
{
		@KineticsFolder		kinetics-C2H5OH-40species;
		@OutputFolder		Output;
		
		@Type							Spark;
		@FlameRadiation         		none;
		
		@FuelMassFractions				C2H5OH 1.;
		@Inert							N2;
		@DropletDiameter				0.7 mm;
		@DropletTemperature				300  K;
		
		@SparkTemperature				2000 K;
		
		@GasStatus          			Initial-Composition;
		
		@DropletPoints					30;
		@GasPoints						100;
		
		@EndTime						100 s;
		
		@FirstGuessInterfaceMassFractions 	C2H5OH 0.197;
		@FirstGuessInterfaceTemperature     299.7 K;
		@FirstGuessInterfaceGasVelocity     176. cm/s;
		
		@StepsVideo						10;
		@StepsMap1D						10;	
		@StepsMap3D						30;
		
		@SnapshotTimes					1e-4 1e-3 1e-2 1e-1;		
		@SnapshotSpecies				C2H5OH O2 CO2 H2O CO OH CH2O H2;
		
		@DAERelativeTolerance			6e-6;
		@NLSRelativeTolerance			1e-10;
}

Dictionary Initial-Composition
{
		@Temperature   	300.    K ; 
		@Pressure      	1. 		bar ;

		@MoleFractions  N2  0.79	
						O2  0.21;
}
